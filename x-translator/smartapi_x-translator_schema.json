{
  "title": "x-translator extension within info.",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "x-translator is a JSON object describes API-level, Translator-specific information. It is an extension/additional property inside the OpenAPI Info Object",
  "properties": {
    "info": {
      "type": "object",
      "properties": {
        "x-translator": {
          "type": "object",
          "required": [
            "api_type",
            "team"
          ],
          "properties": {
            "api_type": {
              "type": "string",
              "enum": [
                "KP",
                "ARA",
                "ARS",
                "SRI Service"
              ]
            },
            "team": {
              "type": "array",
              "minItems": 1,
              "uniqueItems": true,
              "items": {
                "type": "string",
                "enum": [
                  "Service Provider",
                  "Multiomics Provider",
                  "Clinical Data Provider",
                  "Connections Hypothesis Provider",
                  "Molecular Data Provider",
                  "Text Mining Provider",
                  "Exposures Provider",
                  "Genetics Provider",
                  "Ranking Agent",
                  "imProve Agent",
                  "Unsecret Agent",
                  "Explanatory Agent",
                  "Expander Agent",
                  "Exploring Agent",
                  "Link Brokers",
                  "Standards Reference Implementation Team"
                ]
              }
            }
          }
        }
      }
    }
  }
}